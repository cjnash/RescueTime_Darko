<h1>RescueTime - Darko Edition!</h1>
<br/>
<%= render 'form' %>

<table>
  <tr>
    <th>Running history</th>
  </tr>
<% @darkos.each do |darko| %>
  <tr>
  	<td valign="top"><span class="hesaidwhen"><%= darko.created_at.strftime("%B %d, %I:%M %p") %></span></td>
    <td valign="top"><span class="hesaidwhat">&#8220;<%= darko.status %>&#8221;</span><br/></td>
  </tr>
</table>
<% end %>